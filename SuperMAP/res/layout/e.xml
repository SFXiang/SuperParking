<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:custom="http://schemas.android.com/apk/res/com.example.supermap"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#A6FFA6" >

    <com.example.common.TopBar
        android:id="@+id/topbar"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        custom:leftBackground="@color/nav_bar_bg"
        custom:leftText="返回"
        custom:leftTextColor="#FFFFFF"
        custom:tittle="教八楼后侧停车场"
        custom:tittleTextColor="#FFFFFF"
        custom:tittleTextSize="6sp" >
    </com.example.common.TopBar>

    <LinearLayout
        android:id="@+id/road"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="120dp"
        android:background="@drawable/road_heng"
        android:orientation="horizontal" >
    </LinearLayout>

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView1"
        android:layout_marginRight="32dp"
        android:layout_toLeftOf="@+id/textView1"
        android:src="@drawable/shu" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="18dp"
        android:text="教    八    楼     后"
        android:textColor="@color/black"
        android:textSize="20dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView1"
        android:layout_centerHorizontal="true"
        android:src="@drawable/shu" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView2"
        android:layout_marginLeft="18dp"
        android:layout_toRightOf="@+id/textView1"
        android:src="@drawable/shu" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_below="@+id/topbar"
        android:layout_marginBottom="140dp"
        android:layout_marginLeft="30dp"
        android:orientation="vertical" >

        <Button
            android:id="@+id/E01"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_marginTop="100dp"
            android:background="@drawable/car_heng"
            />

        <Button
            android:id="@+id/E02"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/car_heng"
            />

        <Button
            android:id="@+id/E03"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/car_heng"
            />

        <Button
            android:id="@+id/E04"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/car_heng"
           />

        <Button
            android:id="@+id/E05"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/car_heng"
          />

        <Button
            android:id="@+id/E06"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/car_heng"
            />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_below="@+id/topbar"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="140dp"
        android:orientation="vertical" >

        <Button
            android:id="@+id/E07"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_marginTop="100dp"
            android:background="@drawable/car_heng"
            />

        <Button
            android:id="@+id/E08"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/car_heng"
          />

        <Button
            android:id="@+id/E09"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/car_heng"
           />

        <Button
            android:id="@+id/E10"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/car_heng"
           />

        <Button
            android:id="@+id/E11"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/car_heng"
            />

        <Button
            android:id="@+id/E12"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/car_heng"
          />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_above="@+id/road"
        android:layout_alignLeft="@+id/imageView3"
        android:layout_below="@id/topbar"
        android:layout_marginLeft="17dp"
        android:orientation="vertical" >

        <Button
            android:id="@+id/E13"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_marginTop="100dp"
            android:background="@drawable/car_heng"
           />

        <Button
            android:id="@+id/E14"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/car_heng"
          />

        <Button
            android:id="@+id/E15"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/car_heng"
         />

        <Button
            android:id="@+id/E16"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/car_heng"
         />

        <Button
            android:id="@+id/E17"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/car_heng"
            />

        <Button
            android:id="@+id/E18"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/car_heng"
            />
    </LinearLayout>

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/linearLayout2"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="52dp"
        android:layout_toRightOf="@+id/linearLayout2"
        android:src="@drawable/shu" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView4"
        android:layout_toLeftOf="@+id/linearLayout3"
        android:src="@drawable/shu" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageView5"
        android:layout_alignLeft="@+id/imageView4"
        android:src="@drawable/shu" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageView5"
        android:layout_alignLeft="@+id/imageView5"
        android:src="@drawable/shu" />

</RelativeLayout>